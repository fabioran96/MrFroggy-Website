{
  "compilerOptions": {
    "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types", "./types"],
    "types": ["react", "react-dom", "react-bootstrap"]
  },
  "include": ["src/**/*"]
  }
}
